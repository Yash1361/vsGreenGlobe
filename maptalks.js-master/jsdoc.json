{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "templates" : {
        "github" : "https://github.com/maptalks/maptalks.js/tree/master/src/",
        "categories" : "map, layer, geometry, basic types ,maptool, ui, control",
        "default" : {
            "useLongnameInNav" : true
        }
    },

    "opts": {
        "recurse" : true,
        "template": "./node_modules/maptalks-jsdoc/",
        "encoding": "utf8",
        "destination": "./docs/api/",
        "access"  : "protected, undefined"
    }
}
